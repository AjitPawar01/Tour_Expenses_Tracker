<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Outer Ring -->
    <path
        android:pathData="M54,54m-24,0a24,24 0,1 1,48 0a24,24 0,1 1,-48 0"
        android:strokeWidth="2"
        android:fillColor="#00000000">
        <aapt:attr name="android:strokeColor">
            <gradient
                android:startX="30"
                android:startY="30"
                android:endX="78"
                android:endY="78"
                android:type="linear">
                <item android:offset="0" android:color="#FF10B981"/>
                <item android:offset="0.5" android:color="#FF06B6D4"/>
                <item android:offset="1" android:color="#FF6366F1"/>
            </gradient>
        </aapt:attr>
    </path>

    <!-- Central Symbol - Combined Plane and Dollar -->
    <group android:translateX="54" android:translateY="54">

        <!-- Stylized Plane Body -->
        <path
            android:pathData="M0,-12 L-8,0 L-8,2 L-2,0 L0,4 L2,0 L8,2 L8,0 L0,-12">
            <aapt:attr name="android:fillColor">
                <gradient
                    android:startX="-8"
                    android:startY="-12"
                    android:endX="8"
                    android:endY="4"
                    android:type="linear">
                    <item android:offset="0" android:color="#FFE0F2FE"/>
                    <item android:offset="1" android:color="#FFBAE6FD"/>
                </gradient>
            </aapt:attr>
        </path>

        <!-- Dollar Integration -->
        <path
            android:pathData="M0,6 L0,14 M-3,8 Q-3,6 0,6 Q3,6 3,8 Q3,10 0,10 Q-3,10 -3,12 Q-3,14 0,14 Q3,14 3,12"
            android:strokeWidth="2"
            android:strokeLineCap="round"
            android:fillColor="#00000000">
            <aapt:attr name="android:strokeColor">
                <gradient
                    android:startX="-3"
                    android:startY="6"
                    android:endX="3"
                    android:endY="14"
                    android:type="linear">
                    <item android:offset="0" android:color="#FF10B981"/>
                    <item android:offset="1" android:color="#FF059669"/>
                </gradient>
            </aapt:attr>
        </path>

        <!-- Orbit Lines -->
        <path
            android:pathData="M-16,-8 Q-8,-12 0,-8 Q8,-12 16,-8"
            android:strokeColor="#4D818CF8"
            android:strokeWidth="1"
            android:strokeLineCap="round"
            android:fillColor="#00000000"/>

        <path
            android:pathData="M-16,8 Q-8,12 0,8 Q8,12 16,8"
            android:strokeColor="#4D818CF8"
            android:strokeWidth="1"
            android:strokeLineCap="round"
            android:fillColor="#00000000"/>
    </group>

    <!-- Corner Accents -->
    <circle
        android:pathData="M38,38m-1,0a1,1 0,1 1,2 0a1,1 0,1 1,-2 0"
        android:fillColor="#CCFFD700"/>

    <circle
        android:pathData="M70,38m-1,0a1,1 0,1 1,2 0a1,1 0,1 1,-2 0"
        android:fillColor="#CCFFD700"/>

    <circle
        android:pathData="M38,70m-1,0a1,1 0,1 1,2 0a1,1 0,1 1,-2 0"
        android:fillColor="#CCFFD700"/>

    <circle
        android:pathData="M70,70m-1,0a1,1 0,1 1,2 0a1,1 0,1 1,-2 0"
        android:fillColor="#CCFFD700"/>

</vector>